{
  "name": "client-escalation-calls",
  "version": "1.0.0",
  "description": "Agentic AI Payment Collection System for Logistics Companies",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && npm run build:services",
    "build:services": "npm run build:gateway && npm run build:agents && npm run build:engines",
    "build:gateway": "tsc -p src/services/api-gateway/tsconfig.json",
    "build:agents": "tsc -p src/agents/tsconfig.json",
    "build:engines": "tsc -p src/core/engines/tsconfig.json",
    "start": "node dist/index.js",
    "start:dev": "concurrently \"npm run start:gateway\" \"npm run start:agents\" \"npm run start:engines\"",
    "start:gateway": "ts-node src/services/api-gateway/index.ts",
    "start:agents": "ts-node src/agents/coordinator/index.ts",
    "start:engines": "ts-node src/core/engines/index.ts",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests",
    "test:coverage": "jest --coverage --passWithNoTests",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "dev": "nodemon src/index.ts",
    "docker:build": "docker build -t client-escalation-calls .",
    "docker:run": "docker run -p 3000:3000 client-escalation-calls"
  },
  "keywords": [
    "ai",
    "agents",
    "payment-collection",
    "logistics",
    "automation"
  ],
  "author": "Development Team",
  "license": "MIT",
  "dependencies": {
    "@elastic/elasticsearch": "^8.11.0",
    "@qdrant/js-client-rest": "^1.8.2",
    "amqplib": "^0.10.3",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "imap": "^0.8.17",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "kafkajs": "^2.2.4",
    "lodash": "^4.17.21",
    "mailparser": "^3.6.5",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "nodemailer": "^7.0.12",
    "pg": "^8.11.3",
    "prom-client": "^15.1.0",
    "redis": "^4.6.11",
    "twilio": "^4.19.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4",
    "openai": "^4.24.1",
    "@anthropic-ai/sdk": "^0.10.2",
    "@google/generative-ai": "^0.1.3",
    "tsconfig-paths": "^4.2.0"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.4",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/csv-parser": "^1.1.10",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/lodash": "^4.14.202",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.5",
    "@types/nodemailer": "^6.4.14",
    "@types/pg": "^8.10.9",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
